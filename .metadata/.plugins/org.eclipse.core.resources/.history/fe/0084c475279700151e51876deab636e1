		GeneiousActionOptions parent = new GeneiousActionOptions("Name","Description").setMainMenuLocation(GeneiousActionOptions.MainMenu.Tools);
		GeneiousActionOptions submenuItem = new GeneiousActionOptions("Item1", "Description");
		GeneiousActionOptions sub = parent.createSubmenuActionOptions(parent, submenuItem1);
		return sub;
	}

