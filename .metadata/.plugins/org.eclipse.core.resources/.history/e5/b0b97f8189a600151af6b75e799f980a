package com.cipres.mrBayesPlugin.utilities;

import java.util.List;

public class DataHandlingUtilities {
	
	private static DataHandlingUtilities SINGLETON;
	
	public List<Job> jobs;
	
	public class Job{
		
		public String jobName;
		public String data;
		public String jobStage;
		
		public String getJobName() {
			return jobName;
		}
		public void setJobName(String jobName) {
			this.jobName = jobName;
		}
		public String getData() {
			return data;
		}
		public void setData(String data) {
			this.data = data;
		}
		public String getJobStage() {
			return jobStage;
		}
		public void setJobStage(String jobStage) {
			this.jobStage = jobStage;
		}
	}
	
	public static synchronized DataHandlingUtilities getInstance(){
		if(SINGLETON == null)
			SINGLETON = new DataHandlingUtilities();
		return SINGLETON;
	}
	
	private boolean saveData(String filename, String jobName, String date, String jobStage){
		
		return true;
	}
	
	
}
