package com.cipres.mrBayesPlugin;

import com.biomatters.geneious.publicapi.plugin.DocumentOperation;
import com.biomatters.geneious.publicapi.plugin.DocumentSelectionSignature;
import com.biomatters.geneious.publicapi.plugin.GeneiousActionOptions;
import com.biomatters.geneious.publicapi.plugin.Icons;
import com.biomatters.geneious.publicapi.utilities.IconUtilities;

public class CipresMrBayesTree extends DocumentOperation{

	@Override
	public GeneiousActionOptions getActionOptions() {
		Icons icon= IconUtilities.getIcons(arg0);
		GeneiousActionOptions.Category category = GeneiousActionOptions.Category.TreeBuilding : GeneiousActionOptions
        return new GeneiousActionOptions("MrBayes",
                "Perform posterior tree simulation from an alignment", MrBayesPlugin.toolbarIcons, GeneiousActionOptions.Category.TreeBuilding);
	}

	@Override
	public String getHelp() {
		return "Getting Help";
	}

	@Override
	public DocumentSelectionSignature[] getSelectionSignatures() {
		return new DocumentSelectionSignature[0];
	}

}
